{
    "presets": [
        [
            "@babel/preset-env",
            {
              "loose": true,
              "modules": false
            }
        ],
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        "@babel/plugin-syntax-dynamic-import",
        "@babel/plugin-proposal-export-default-from"
    ]
}